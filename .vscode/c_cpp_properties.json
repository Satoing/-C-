{
    "configurations": [
      {
        "name": "windows-gcc-x86",
        "includePath": [
          "${workspaceFolder}/**",
          "C:/MinGW/include/**"
        ],
        "defines": [
          "_DEBUG",
          "UNICODE",
          "_UNICODE"
        ],
        "windowsSdkVersion": "8.1",
        "compilerPath": "C:\\MinGW\\bin\\g++.exe",
        "cStandard": "${default}",
        "cppStandard": "${default}",
        "intelliSenseMode": "windows-gcc-x86",
        "compilerArgs": [
          "-Wall",
          "-Wextra",
          "-Wpedantic"
        ]
      }
    ],
    "version": 4
  }